{
    "version": 2,
    "builds": [
        {
            "src": "unified-server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "unified-server.js"
        }
    ],
    "functions": {
        "unified-server.js": {
            "memory": 1024,
            "maxDuration": 60
        }
    }
}